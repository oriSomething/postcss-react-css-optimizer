{
  "rules": {
    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "markdown",
    "react"
  ]
}
